<%- include('../partials/header') %>

<main class="main" id="allDatMain">
    <div class="container">
        <div class="row-cols-1 row-cols-md-3 row-cols-lg-4">
            data.forEach(item => { %>
                <div class="card h-100">
                    <div class="card-body">
                        <h2 class="card-title card-heading">
                            <% item.<%= title %> %>
                        </h2>
                    </div>
                </div>

            <%  }) %>
        </div>
    </div>
</main>

<%- include('../partials/footer') %>